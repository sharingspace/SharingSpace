{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "declaration": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "removeComments": true,
    "strictNullChecks": false,
    "allowSyntheticDefaultImports": true,
    "outDir": "build",
    "lib": [
      "es6",
      "es7",
      "dom"
    ],
    "baseUrl": "./",
    "paths": {
      "config": ["config/dev.ts", "config/prod.ts"],
      "images": ["src/assets/images/"],
      "stores": ["src/app/stores"],
      "apiCalls": ["src/app/services/apiCalls.ts"],
      "models": ["src/app/models"]
    }
  },
  "exclude": [
    "dist",
    "build",
    "node_modules"
  ],
  "types": [
    "webpack-env"
  ]
}
